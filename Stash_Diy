#混合端口
mixed-port: 7890
#允许局域网
allow-lan: true
#绑定地址
bind-address: '*'
#模式:rule（规则） / global（全局代理）/ direct（全局直连）
mode: Rule
#日志级别（日志输出级别：分级如后silent / info / warning / error / debug级别越高日志输出量越大）
log-level: info
ipv6: false
#外部控制器
external-controller: 127.0.0.1:9090

subscribe-url: 你的订阅链接
# 替换上面文字用于Stash获取机场流量信息并在主页显示


#DNS设置
dns:
  enable: true
  ipv6: false
  listen: 0.0.0.0:1053
  #DNS服务器
  default-nameserver:
    - 119.29.29.29
    - 119.28.28.28
    - 223.5.5.5
    - 114.114.114.114
  #DoH服务器
  nameserver:
    - https://dns.alidns.com/dns-query
    - https://doh.dns.sb/dns-query
  enhanced-mode: fake-ip
  fake-ip-range: 198.18.0.1/16
  #列表中的域名将不会被解析为fake ip，这些域名相关的解析请求将会返回它们真实的IP地址
  fake-ip-filter:
    - '*.lan'
    - localhost.ptlogin2.qq.com
    - '+.srv.nintendo.net'
    - '+.stun.playstation.net'
    - '+.msftconnecttest.com'
    - '+.msftncsi.com'
    - '+.xboxlive.com'
    - 'msftconnecttest.com'
    - 'xbox.*.microsoft.com'
    - '*.battlenet.com.cn'
    - '*.battlenet.com'
    - '*.blzstatic.cn'
    - '*.battle.net'

#本地代理
proxies:

#远程订阅
proxy-providers:
  All-Proxy:
    type: http
    url: "https://sub.store/download/collection/AllServer"
    # 使用Clash类型订阅链接替换掉引号""内的链接使用;或可使用通用订阅链接通过Sub-Store进行订阅转换。
    interval: 86400
    # filter: '节点关键词'
    health-check:
      enable: true
      interval: 600
      url: http://cp.cloudflare.com/generate_204
      


