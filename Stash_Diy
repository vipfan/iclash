# 混合端口，HTTP和SOCKS5用一个端口
mixed-port: 7890
# 允许局域网的连接（共享代理）
allow-lan: true
# 监听IP白名单，可绑定单个IPv4和v6地址，"*" 为绑定所有IP地址，仅在将allow-lan设置为true时适用
bind-address: '*'
# 模式:rule（规则） / global（全局代理）/ direct（全局直连）
mode: Rule
# 日志级别（日志输出级别：分级如后silent / info / warning / error / debug级别越高日志输出量越大）
log-level: info
ipv6: true
# 外部控制器， 可以使用 RESTful API 来控制你的 clash 内核
external-controller: 127.0.0.1:9090

# subscribe-url: 你的订阅链接
# 替换上面文字用于Stash获取机场流量信息并在主页显示


# DNS设置
dns:
  enable: true
  ipv6: true
  listen: 0.0.0.0:1053
  # DNS服务器
  default-nameserver:
    - system
    - 119.29.29.29
    - 223.5.5.5
    - 114.114.114.114
  # DoH服务器
  nameserver:
    - https://dns.alidns.com/dns-query
    - https://doh.pub/dns-query
  # 模式：redir-host或fake-ip
  enhanced-mode: fake-ip
  # Fake-IP 解析地址池
  fake-ip-range: 198.18.0.1/16
  #列表中的域名将不会被解析为fake ip，这些域名相关的解析请求将会返回它们真实的IP地址
  fake-ip-filter:
    - '*.lan'
    - localhost.ptlogin2.qq.com
    - '+.srv.nintendo.net'
    - '+.stun.playstation.net'
    - '+.msftconnecttest.com'
    - '+.msftncsi.com'
    - '+.xboxlive.com'
    - 'msftconnecttest.com'
    - 'xbox.*.microsoft.com'
    - '*.battlenet.com.cn'
    - '*.battlenet.com'
    - '*.blzstatic.cn'
    - '*.battle.net'

# 本地代理
proxies:

# 远程订阅
proxy-providers:
  All-Proxy:
    url: "https://sub.store/download/collection/AllServer"
    # 使用Clash类型订阅链接替换掉引号""内的链接使用;或可使用通用订阅链接通过Sub-Store进行订阅转换。
    interval: 86400
    # filter: '节点关键词'
    health-check:
      enable: true
      interval: 3600
      url: http://cp.cloudflare.com/generate_204
      
